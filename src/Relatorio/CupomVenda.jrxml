	<queryString>
		<![CDATA[Select 
ven_id, ven_valor_total, ven_desconto, ven_forma_pagamento, ven_data, ven_hora, 
fun_nome, fun_id, 
clie_nome, clie_tipo, clie_id,
(ven_valor_total - ven_desconto) AS VALOR_TOTAL
